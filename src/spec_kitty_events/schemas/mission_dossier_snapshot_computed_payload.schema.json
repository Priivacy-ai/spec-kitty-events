{
  "$defs": {
    "LocalNamespaceTuple": {
      "description": "Minimum collision-safe key for parity baseline scoping.\n\nmanifest_version is defined HERE ONLY \u2014 event payloads must NOT duplicate it.",
      "properties": {
        "feature_slug": {
          "minLength": 1,
          "title": "Feature Slug",
          "type": "string"
        },
        "manifest_version": {
          "minLength": 1,
          "title": "Manifest Version",
          "type": "string"
        },
        "mission_key": {
          "minLength": 1,
          "title": "Mission Key",
          "type": "string"
        },
        "project_uuid": {
          "minLength": 1,
          "title": "Project Uuid",
          "type": "string"
        },
        "step_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Step Id"
        },
        "target_branch": {
          "minLength": 1,
          "title": "Target Branch",
          "type": "string"
        }
      },
      "required": [
        "project_uuid",
        "feature_slug",
        "target_branch",
        "mission_key",
        "manifest_version"
      ],
      "title": "LocalNamespaceTuple",
      "type": "object"
    }
  },
  "$id": "spec-kitty-events/mission_dossier_snapshot_computed_payload",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Payload for MissionDossierSnapshotComputed events.\n\nmanifest_version NOT duplicated here \u2014 use namespace.manifest_version.",
  "properties": {
    "algorithm": {
      "anyOf": [
        {
          "enum": [
            "sha256",
            "sha512",
            "md5"
          ],
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Algorithm"
    },
    "anomaly_count": {
      "minimum": 0,
      "title": "Anomaly Count",
      "type": "integer"
    },
    "artifact_count": {
      "minimum": 0,
      "title": "Artifact Count",
      "type": "integer"
    },
    "computed_at": {
      "description": "ISO 8601",
      "title": "Computed At",
      "type": "string"
    },
    "context_diagnostics": {
      "anyOf": [
        {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Context Diagnostics"
    },
    "namespace": {
      "$ref": "#/$defs/LocalNamespaceTuple",
      "description": "manifest_version lives here only"
    },
    "snapshot_hash": {
      "description": "Hex-encoded deterministic hash",
      "minLength": 1,
      "title": "Snapshot Hash",
      "type": "string"
    }
  },
  "required": [
    "namespace",
    "snapshot_hash",
    "artifact_count",
    "anomaly_count",
    "computed_at"
  ],
  "title": "MissionDossierSnapshotComputedPayload",
  "type": "object"
}
