{
  "$id": "spec-kitty-events/local_namespace_tuple",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Minimum collision-safe key for parity baseline scoping.\n\nmanifest_version is defined HERE ONLY \u2014 event payloads must NOT duplicate it.",
  "properties": {
    "feature_slug": {
      "minLength": 1,
      "title": "Feature Slug",
      "type": "string"
    },
    "manifest_version": {
      "minLength": 1,
      "title": "Manifest Version",
      "type": "string"
    },
    "mission_key": {
      "minLength": 1,
      "title": "Mission Key",
      "type": "string"
    },
    "project_uuid": {
      "minLength": 1,
      "title": "Project Uuid",
      "type": "string"
    },
    "step_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Step Id"
    },
    "target_branch": {
      "minLength": 1,
      "title": "Target Branch",
      "type": "string"
    }
  },
  "required": [
    "project_uuid",
    "feature_slug",
    "target_branch",
    "mission_key",
    "manifest_version"
  ],
  "title": "LocalNamespaceTuple",
  "type": "object"
}
