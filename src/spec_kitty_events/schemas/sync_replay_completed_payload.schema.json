{
  "$id": "spec-kitty-events/sync_replay_completed_payload",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Payload for SyncReplayCompleted events.",
  "properties": {
    "connector_id": {
      "minLength": 1,
      "title": "Connector Id",
      "type": "string"
    },
    "delivery_id": {
      "minLength": 1,
      "title": "Delivery Id",
      "type": "string"
    },
    "mission_id": {
      "minLength": 1,
      "title": "Mission Id",
      "type": "string"
    },
    "recorded_at": {
      "format": "date-time",
      "title": "Recorded At",
      "type": "string"
    },
    "replay_id": {
      "minLength": 1,
      "title": "Replay Id",
      "type": "string"
    },
    "replay_source": {
      "minLength": 1,
      "title": "Replay Source",
      "type": "string"
    },
    "replayed_count": {
      "minimum": 0,
      "title": "Replayed Count",
      "type": "integer"
    },
    "source_event_fingerprint": {
      "minLength": 1,
      "title": "Source Event Fingerprint",
      "type": "string"
    }
  },
  "required": [
    "delivery_id",
    "source_event_fingerprint",
    "connector_id",
    "mission_id",
    "recorded_at",
    "replay_id",
    "replayed_count",
    "replay_source"
  ],
  "title": "SyncReplayCompletedPayload",
  "type": "object"
}
